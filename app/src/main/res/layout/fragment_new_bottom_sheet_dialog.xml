<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ll_dialog_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_custom_bottom_sheet_dialog_fragment"
    android:backgroundTint="#00002C"
    android:orientation="vertical"
    tools:context=".NewBottomSheetDialogFragment">

    <com.google.android.material.card.MaterialCardView
        android:layout_width="80dp"
        android:layout_height="4dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginVertical="8dp"
        app:cardBackgroundColor="#8087A9"
        app:cardCornerRadius="2dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginVertical="32dp"
        android:text="Testing"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="18sp" />
</LinearLayout>